{
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "cli": { "packageManager": "pnpm" },
    "targetDefaults": {
        "print-pwd": {
            "executor": "nx:run-commands",
            "options": {
                "commands": [
                    "pwd"
                ],
                "color": true,
                "cwd": "{projectRoot}"
            } 
        }
    }
}